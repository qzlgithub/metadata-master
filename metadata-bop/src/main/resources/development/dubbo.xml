<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://code.alibabatech.com/schema/dubbo
            http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 应用信息，用于计算依赖关系 -->
    <dubbo:application name="METADATA-BOP" />

    <!-- 注册中心服务地址 -->
    <dubbo:registry id="zookeeper" protocol="zookeeper" address="${dubbo.registry.address}" />

    <!--***************************************************************************-->
    <!-- consumer配置：生成远程服务代理，和本地bean一样调用 -->
    <dubbo:reference retries="0" timeout="5000" check="false" id="productRpcService" url="dubbo://192.168.2.232:33002/"
                     interface="com.mingdong.core.service.ProductRpcService" />
    <dubbo:reference retries="0" timeout="5000" check="false" id="clientRpcService" url="dubbo://192.168.2.232:33002/"
                     interface="com.mingdong.core.service.ClientRpcService" />
    <dubbo:reference retries="0" timeout="5000" check="false" id="managerRpcService" url="dubbo://192.168.2.232:33002/"
                     interface="com.mingdong.core.service.ManagerRpcService" />
    <dubbo:reference retries="0" timeout="5000" check="false" id="systemRpcService" url="dubbo://192.168.2.232:33002/"
                     interface="com.mingdong.core.service.SystemRpcService" />
    <dubbo:reference retries="0" timeout="5000" check="false" id="commonRpcService" url="dubbo://192.168.2.232:33002/"
                     interface="com.mingdong.core.service.CommonRpcService"/>
    <dubbo:reference retries="0" timeout="5000" check="false" id="tradeRpcService" url="dubbo://192.168.2.232:33002/"
                     interface="com.mingdong.core.service.TradeRpcService"/>
    <dubbo:reference retries="0" timeout="5000" check="false" id="trafficService" url="dubbo://192.168.2.232:33009/"
                     interface="com.mingdong.backend.service.TrafficService"/>
    <dubbo:reference retries="0" timeout="5000" check="false" id="dataStatsService" url="dubbo://192.168.2.232:33009/"
                     interface="com.mingdong.backend.service.DataStatsService"/>
    <dubbo:reference retries="0" timeout="5000" check="false" id="warningService" url="dubbo://192.168.2.232:33009/"
                     interface="com.mingdong.backend.service.WarningService"/>
</beans>