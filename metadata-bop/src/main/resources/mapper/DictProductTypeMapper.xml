<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mingdong.bop.domain.mapper.DictProductTypeMapper">
    <resultMap id="BaseResultMap" type="DictProductType">
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
        <result column="code" property="code" jdbcType="VARCHAR" />
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="remark" property="remark" jdbcType="VARCHAR" />
        <result column="enabled" property="enabled" jdbcType="TINYINT" />
    </resultMap>

    <sql id="select_all">
        select
            `id`, `create_time`, `update_time`, `code`, `name`, `remark`, `enabled`
        from `dict_product_type`
    </sql>

    <insert id="add" parameterType="DictProductType">
        insert into `dict_product_type`
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">`id`,</if>
            <if test="createTime != null">`create_time`,</if>
            <if test="updateTime != null">`update_time`,</if>
            <if test="code != null">`code`,</if>
            <if test="name != null">`name`,</if>
            <if test="remark != null">`remark`,</if>
            <if test="enabled != null">`enabled`,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="code != null">#{code},</if>
            <if test="name != null">#{name},</if>
            <if test="remark != null">#{remark},</if>
            <if test="enabled != null">#{enabled},</if>
        </trim>
    </insert>

    <update id="updateById" parameterType="DictProductType">
        update `dict_product_type`
        set `create_time` = #{createTime},
            `update_time` = #{updateTime},
            `code`        = #{code},
            `name`        = #{name},
            `remark`      = #{remark},
            `enabled`     = #{enabled}
        where `id` = #{id}
    </update>

    <update id="updateSkipNull" parameterType="DictProductType">
        update `dict_product_type`
        <set>
            <if test="createTime != null">`create_time` = #{createTime},</if>
            <if test="updateTime != null">`update_time` = #{updateTime},</if>
            <if test="code != null">`code` = #{code},</if>
            <if test="name != null">`name` = #{name},</if>
            <if test="remark != null">`remark` = #{remark},</if>
            <if test="enabled != null">`enabled` = #{enabled},</if>
        </set>
        where `id` = #{id}
    </update>

    <select id="getAll" resultMap="BaseResultMap">
        select
            `id`, `create_time`, `update_time`, `code`, `name`, `remark`, `enabled`
        from `dict_product_type`
        order by `create_time` desc
    </select>

    <select id="findById" parameterType="Long" resultMap="BaseResultMap">
        select
            `id`, `create_time`, `update_time`, `code`, `name`, `remark`, `enabled`
        from `dict_product_type`
        where `id` = #{id}
    </select>

    <select id="findByCode" resultType="DictProductType">
        select
            `id`, `create_time`, `update_time`, `code`, `name`, `remark`, `enabled`
        from `dict_product_type`
        where `code` = #{code}
    </select>

    <select id="countAll" resultType="Integer">
        select count(*)
        from dict_product_type
    </select>

    <select id="getListByStatus" parameterType="Integer" resultMap="BaseResultMap">
        <include refid="select_all" />
        <where>
            <if test="enabled != null">`enabled` = #{enabled}</if>
        </where>
    </select>
</mapper>